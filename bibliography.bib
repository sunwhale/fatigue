doi = {10.1016/j.msea.2015.12.096},
file = {:F$\backslash$:/Articles/Fatigue/Papers/Chen et al. - 2016 - Low cycle fatigue and creep-fatigue interaction behavior of nickel-base superalloy GH4169 at elevated temperature.pdf:pdf},
issn = {09215093},
