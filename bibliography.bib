issn = {01421123},
journal = {International Journal of Fatigue},
keywords = {inco718,induction heating,pyrometer control,strain control fatigue,thermo-mechanical fatigue},
number = {2},
pages = {257--267},
title = {{Thermo-mechanical fatigue and fracture of INCO718}},
url = {http://www.sciencedirect.com/science/article/pii/S0142112307001120 http://ac.els-cdn.com/S0142112307001120/1-s2.0-S0142112307001120-main.pdf?{\_}tid=5246cdc2-9cbb-11e6-bb6a-00000aab0f6b{\&}acdnat=1477624083{\_}4a86d2d119551aea4d5c93cd40e93275},
