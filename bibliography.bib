keywords = {Creep-fatigue interaction,GH4169,Low cycle fatigue,Nickel based superalloys},
pages = {175--182},
title = {{Low cycle fatigue and creep-fatigue interaction behavior of nickel-base superalloy GH4169 at elevated temperature of 650Â°C}},
volume = {655},
year = {2016}
}
